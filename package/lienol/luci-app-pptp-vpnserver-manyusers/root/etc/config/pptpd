
config service 'pptpd'
	option mppe '1'
	option localip '192.168.2.1'
	option remoteip '192.168.2.10-20'
	option dns '192.168.0.2'
	option is_nat '1'
	option export_interface 'default'
	option enabled '0'

config users
	option enabled '1'
	option ipaddress '*'
	option username 'guest'
	option password '123456'

